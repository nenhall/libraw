{
  "name": "libraw",
  "version": "0.0.1",
  "summary": "A short description of PixRaw.",
  "description": "PixRaw is a library for processing raw image data.",
  "homepage": "https://techgit.meitu.com/mtgtech/zero-team/pixengine-rendercore",
  "license": {
    "type": "MIT",
    "file": "LICENCE"
  },
  "authors": {
    "Nenghao Wu": "nenghao.wu@pixocial.com"
  },
  "platforms": {
    "ios": "12.0",
    "osx": "10.15"
  },
  "source": {
    "git": "git@techgit.meitu.com:mtgtech/zero-team/pixengine-rendercore.git",
    "tag": "#{spec.version}"
  },
  "source_files": [
    "libraw/src/*.{cpp}",
    "libraw/src/**/*.{h,cpp}",
    "libraw/libraw/*.{h}",
    "libraw/internal/*.h"
  ],
  "exclude_files": "libraw/src/**/*_ph.cpp",
  "public_header_files": "libraw/libraw/*.h",
  "header_dir": "internal",
  "libraries": "z",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(SRCROOT)/libraw\""
  }
}
